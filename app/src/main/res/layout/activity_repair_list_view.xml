<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="appewtc.masterung.drivingbetter.RepairListView"
    android:background="@drawable/bg480">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fix"
        android:id="@+id/textView39"
        android:textSize="@dimen/h2"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:textColor="@color/text2" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView"
        android:layout_below="@+id/textView39"
        android:layout_margin="15dp"
        android:background="#96ffffff"
        android:layout_above="@+id/button5" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="clickBackRepairListView"
        android:text="Back"
        android:id="@+id/button5"
        android:layout_alignParentBottom="true" />
</RelativeLayout>
